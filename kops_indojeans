kops create cluster \
  --name=kops.indojeans.in \
  --state=s3://kops-indojeans-state-store \
  --authorization RBAC \
  --zones=ap-south-1a \
  --node-count=2 \
  --node-size=t2.micro \
  --master-size=t2.micro \
  --master-count=1 \
  --dns-zone=indojeans.in \
  --out=kops_terraform \
  --target=terraform \
  --ssh-public-key=~/.ssh/kops_indojeans.pub
